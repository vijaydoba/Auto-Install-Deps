{
  "name": "auto-install-deps",
  "displayName": "Auto Install Deps",
  "description": "Detect missing dependencies for multiple languages and install them with one click.",
  "version": "0.1.1",
  "publisher": "vijaydoba",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png", 
  "repository": {
    "type": "git",
    "url": "https://github.com/vijaydoba/Auto-Install-Deps.git"
  },
  "bugs": {
    "url": "https://github.com/vijaydoba/Auto-Install-Deps/issues"
  },
  "homepage": "https://github.com/vijaydoba/Auto-Install-Deps",
  "license": "MIT",
  "main": "./dist/extension.js",
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:python",
    "onLanguage:java",
    "onLanguage:csharp",
    "onLanguage:php",
    "onLanguage:go",
    "onLanguage:rust",
    "onLanguage:ruby",
    "onLanguage:dart"
  ],
  "contributes": {
    "commands": [
      {
        "command": "auto-install-deps.installDependency",
        "title": "Auto Install Deps: Install Dependency"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.96.0",
    "typescript": "^5.6.0"
  }
}
